project {
  requires += dotnet
  after += QuickFASTDotNet
  includes += $(QUICKFAST_ROOT)/src
  specific(!vc8, !vc9) {
    libpaths += $(QUICKFAST_ROOT)/lib
  }
  specific(vc8, vc9) {
    Release::libpaths += $(QUICKFAST_ROOT)/Output/Release
    Debug::libpaths += $(QUICKFAST_ROOT)/Output/Debug  
  }
}
