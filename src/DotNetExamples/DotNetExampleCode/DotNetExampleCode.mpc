project(*): QuickFASTDotNetApplication {
  requires += dotnet
  sharedname = DotNetExampleCode
  libs += QuickFASTDotNet
  after += QuickFASTDotNet

  specific(vc8, vc9) {
    Release::exeout = $(QUICKFAST_ROOT)/Output/Release
    Debug::exeout = $(QUICKFAST_ROOT)/Output/Debug
  }

  managed = 1

  specific {
    guid = FC6DC70F-756E-48B7-9768-1E1EE2B5615F
  }

  Source_Files {
    Properties{
      Properties\AssemblyInfo.cs
    }
    *.cs
  }
}

